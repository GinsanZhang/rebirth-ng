{
  "name": "rebirth-ng",
  "version": "0.0.10",
  "license": "MIT",
  "angular-cli": {},
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "ng": "ng",
    "i18n:extract": "ng-xi18n",
    "start": "npm-run-all --parallel client:run server:run",
    "client:run": "ng serve -o",
    "server:run": "nodemon  ./build/mock.server.js",
    "build": "ng build",
    "test": "npm run lint && ng test --watch=false --single-run=true --code-coverage && npm run npm:publish && rm -rf ./lib",
    "lint": "ng lint",
    "lint-fix": "tslint -c tslint.json src/**/*.ts --fix",
    "e2e": "ng e2e",
    "typedoc": "typedoc",
    "typedocs": "npm run typedoc -- --options typedoc.json  --exclude '**/*.spec.ts' ./src/app/exports/",
    "compodocs:serve": "compodoc ./src/app/exports/ -p ./src/tsconfig.json -b / -d ./compodocs/ --hideGenerator -s",
    "compodocs:gen": "compodoc ./src/app/exports/ -p ./src/tsconfig.app.json -b /rebirth-ng/compodocs/ -d ./dist/compodocs/ --hideGenerator",
    "demo:gen": "ng build --prod --aot --base-href=/rebirth-ng/ && gulp optimize",
    "github-pages:push": "ngh --message \"rebirth ng demo deploy\"",
    "sw:gen": "gulp sw:gen",
    "docs:deploy": "npm run demo:gen && npm run compodocs:gen && npm run sw:gen && npm run github-pages:push && rm -rf ./dist",
    "npm:publish": "rm -rf ./dist && gulp prepublish"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greengerong/rebirth-ng.git"
  },
  "devDependencies": {
    "@angular/animations": "4.1.0-beta.1",
    "@angular/cli": "^1.0.0",
    "@angular/common": "4.1.0-beta.1",
    "@angular/compiler": "4.1.0-beta.1",
    "@angular/compiler-cli": "4.1.0-beta.1",
    "@angular/core": "4.1.0-beta.1",
    "@angular/forms": "4.1.0-beta.1",
    "@angular/http": "4.1.0-beta.1",
    "@angular/platform-browser": "4.1.0-beta.1",
    "@angular/platform-browser-dynamic": "4.1.0-beta.1",
    "@angular/platform-server": "^4.1.0-beta.1",
    "@angular/router": "4.1.0-beta.1",
    "@compodoc/compodoc": "^1.0.0-beta.3",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.62",
    "@types/node": "^6.0.60",
    "@types/webpack": "^2.2.7",
    "angular-cli-ghpages": "^0.5.0",
    "angular-ssr": "^0.1.77",
    "angular2-inline-template-style": "^1.0.0",
    "body-parser": "^1.17.1",
    "bootstrap-sass": "^3.3.7",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "~2.0.0",
    "core-js": "^2.4.1",
    "cross-env": "^4.0.0",
    "del": "^2.2.2",
    "drag-drop-webkit-mobile": "^1.4.1",
    "express": "^4.15.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-ejs": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-insert-lines": "0.0.4",
    "gulp-optimize-js": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "highlight.js": "^9.9.0",
    "html-loader": "^0.4.4",
    "immutable": "^4.0.0-rc.1",
    "intl": "^1.2.5",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.2.1",
    "markdown-loader": "^2.0.0",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "protractor": "~5.1.0",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "sw-precache": "^4.3.0",
    "ts-helpers": "^1.1.1",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typedoc": "^0.5.5",
    "typescript": "^2.2.2",
    "web-animations-js": "^2.2.2",
    "zone.js": "0.8.4"
  },
  "dependencies": {
    "date-fns": "^1.27.2"
  },
  "description": "> Angular4 UI plugin with bootstrap;",
  "bugs": {
    "url": "https://github.com/greengerong/rebirth-ng/issues"
  },
  "homepage": "https://github.com/greengerong/rebirth-ng#readme",
  "author": ""
}

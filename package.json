{
  "name": "rebirth-ui",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "ngc": "ngc -p ./src/tsconfig-aot.json",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "typedoc": "typedoc",
    "typedocs": "npm run typedoc -- --options typedoc.json  --exclude '**/*.spec.ts' ./src/app/exports/",
    "compodocs:serve": "node_modules/.bin/compodoc ./src/app/exports/ -p ./src/tsconfig.json -b / -d ./compodocs/ --hideGenerator -s",
    "compodocs:deploy": "node_modules/.bin/compodoc ./src/app/exports/ -p ./src/tsconfig.json -b /rebirth-ui/compodocs/ -d ./dist/compodocs/ --hideGenerator",
    "demo:deploy": "ng github-pages:deploy  --message \"rebirth ui demo deploy\"",
    "rebith-docs:deploy": "bash ./build/rebith-docs-deploy.sh"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/greengerong/rebirth-ui"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.30",
    "@angular/common": "^2.4.6",
    "@angular/compiler": "^2.4.6",
    "@angular/compiler-cli": "^2.4.6",
    "@angular/core": "^2.4.6",
    "@angular/forms": "^2.4.6",
    "@angular/http": "^2.4.6",
    "@angular/platform-browser": "^2.4.6",
    "@angular/platform-browser-dynamic": "^2.4.6",
    "@angular/router": "^3.4.6",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "bootstrap-sass": "^3.3.7",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "~2.0.0-beta.1",
    "compodoc": "0.0.38",
    "core-js": "^2.4.1",
    "highlight.js": "^9.9.0",
    "html-loader": "^0.4.4",
    "intl": "^1.2.5",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "markdown-loader": "^2.0.0",
    "npm-run-all": "^4.0.1",
    "protractor": "~5.1.0",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.2.1",
    "tslint": "^4.4.2",
    "typedoc": "^0.5.5",
    "typescript": "~2.1.5",
    "web-animations-js": "^2.2.2",
    "zone.js": "0.7.4"
  },
  "dependencies": {
    "date-fns": "^1.27.2"
  }
}
